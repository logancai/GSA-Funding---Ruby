                            </ul>
                        </li>
                        <li><a href="./direct-link-url1">VOLUPTATEM</a></li>
                        <li><a href="./direct-link-url2">ACCUSANTIUM</a></li>
                        <li><a href="./direct-link-url3">DOLOREMQUE</a></li>
                    </ul><!-- End Main Nav -->            
                    <div id="dropdown-spacer"></div> 
                </div><!-- End Nav --> 
            </div><!-- End Nav Group -->           
        </div>
        <!-- End Header Content -->
</div>
<!-- End Header -->
<!-- Container -->
<div id="container">

	<!-- Banner & Nav -->
	<div id="nav-image-holder">
		<img src="/assets/960x450.jpg" id="hero-image" alt="Placeholder" />
	</div>
	<!-- End Banner & Nav -->	
	
	<!-- Main -->
	<div id="main-content" role="main">
	<%= yield %>
	</div>
	<!-- End Container -->
	<div id="footer-push"></div>


</div>
<!-- End Page Wrap -->
